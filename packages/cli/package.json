{
  "private": true,
  "name": "@frostleaf/cli",
  "version": "0.1.0",
  "scripts": {
    "check": "tsc",
    "build": "esbuild --platform=node --bundle --outfile=dist/main.js --external:esbuild src/main.ts",
    "main": "node dist/main.js",
    "format": "prettier --write ./src"
  },
  "bin": {
    "frostleaf": "./bin/frostleaf.js"
  },
  "dependencies": {
    "esbuild": "0.14.42"
  },
  "devDependencies": {
    "@frostleaf/build-time": "workspace:../build-time",
    "@types/estree": "0.0.51",
    "@types/node": "17.0.36",
    "arg": "5.0.1",
    "chalk": "5.0.1",
    "prettier": "2.6.2",
    "typescript": "4.7.2"
  }
}
